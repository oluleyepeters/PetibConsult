<%- include ./partials/header %>
    <title>2D and 3D Designs: Home. Landscape, Offices etc</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="Petib Login">
    <meta name="description" content="Register with us and have a better feeling of our website">
<%- include ./partials/headers2 %>
<%- include ./partials/headers3 %>
<div class="container">
<div class="w-50 mt-3 mx-auto b-2">
<h5 class='text-center'>Login</h5>
<form action="/login" method='POST'>
    <div class="form-group">
    <label for="logemail">Email</label>
    <br>
    <input type="text" class="form-control" name="email" placeholder="Email" id="logemail">
    </div>
    <div class="form-group">
    <label for="logpword">Password</label>
    <br>
    <input type="password" class="form-control" name="password" placeholder="password" id="logpword">
    </div>
    <div class="form-group">
    <button class="btn btn-sm nav-bg btn-block" id="logbutton" type="submit">Login</button>
    <p class="text-center text-sm mb-2"><a href="/forgotpassword">Forget Password ?</a></p>
    <p  class="text-center mb-2">Dont have an account yet? <a href="/register">Sign Up</a></p>
    
    </div>
</form>
</div>
</div>
<%- include ./partials/footer %>